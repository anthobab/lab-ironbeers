{{#each randomBeer}}
  {{! image, name, description and tagline }}
  <div>
    {{> beerpartial this}} 
    <h3>Food Pairing</h3>
    <p><ul>
        {{#each this.food_pairing}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
    </p>
    <p><h3>Brewers Tips : </h3> {{this.brewers_tips}} </p>
  </div>

{{/each}}